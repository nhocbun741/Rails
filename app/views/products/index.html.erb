<table>
<tr>
    <th>Title</th>
    <th>Description</th>
    <th>Price</th>
    <th>Titile of Product</th>
</tr>
<% @products.each do |product| %>
<tr>
    <td><%= product.title%></td>
    <td><%= product.description%></td>
    <td><%= product.price%></td>
    <td><%= product.category.try(:title)%></td>
</tr>
<% end %>
  </table>